[sub_resource type="GDScript" id=65]
script/source = "# Author: Marcus

extends Area2D

var _activated: bool = false
var _timer: float = 0

const TRANSITION_TIME = 2.5


# Called when the node enters the scene tree for the first time.
func _ready():
	self.connect(\"body_entered\", self, \"_on_body_entered\")

func _on_body_entered(body: KinematicBody2D):
	if body is Player:
		self._activated = true
		
func _process(delta):
	if not self._activated or self._timer > TRANSITION_TIME:
		return
	self._timer += delta
	var interp = MathUtils.interpolate(self._timer / TRANSITION_TIME, 1, 0.3, MathUtils.INTERPOLATE_OUT)
	var alpha = MathUtils.interpolate(self._timer / TRANSITION_TIME, 0, 1, MathUtils.INTERPOLATE_OUT_QUAD)
	Scene.ui.get_node(\"VisionReduction\").material.set_shader_param(\"mask_radius\",interp)
	Scene.ui.get_node(\"VisionReduction2\").material.set_shader_param(\"mask_radius\",interp)
	Scene.ui.get_node(\"VisionReduction\").modulate.a = alpha
	Scene.ui.get_node(\"VisionReduction2\").modulate.a = alpha
		

"

