uniform vec2 target = vec2(0.5);
uniform float radius = 0.5;

void fragment(){
	COLOR = texture(TEXTURE,UV);
	float dist = abs(distance(target,SCREEN_UV));
	float strenth = mix(0,1,dist/radius);
	if(dist<=radius && COLOR.a>0.0){
		COLOR.a = strenth;
	}
}
"

